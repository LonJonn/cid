{
  "name": "cid",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "prisma": {
    "schema": "packages/server/prisma/schema.prisma",
    "seed": "tsx packages/server/src/seed.ts"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate:deploy": "turbo run db:migrate:deploy",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "generate": "turbo run generate"
  },
  "devDependencies": {
    "@cid/eslint-config": "workspace:*",
    "prettier": "^2.7.1",
    "tsx": "^3.12.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.12.2",
  "pnpm": {
    "patchedDependencies": {
      "@eslint/eslintrc@1.3.3": "patches/@eslint__eslintrc@1.3.3.patch"
    }
  }
}
