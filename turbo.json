{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV", "DATABASE_URL", "EVENT_STAGE", "ADMIN_LOCKED"],
  "pipeline": {
    "build": {
      "dependsOn": ["^generate", "^build"],
      "outputs": ["dist/**", ".next/**", "api/**", "public/build/**"]
    },
    "dev": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^generate"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["typecheck"],
      "outputs": []
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "generate": {
      "cache": false
    }
  }
}
